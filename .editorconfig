# https://editorconfig.org

root = true

[*]
charset = utf-8
indent_style = space
# https://github.com/microsoft/vscode/issues/6447#issuecomment-221075881
# indent_size = 2
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
